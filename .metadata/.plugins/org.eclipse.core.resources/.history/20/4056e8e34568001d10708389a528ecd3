package uninter;

public class Euro extends Moeda{
	

	public Euro(double valor) {
		super(valor);
		this.valor = valor;
	}

	@Override
	public void info() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void converter() {
		valor =valor*6;
		
	}
	@Override
	public String toString() {
		return "Euro [valor= Ð„$ " + valor + "]";
	}
	

}
