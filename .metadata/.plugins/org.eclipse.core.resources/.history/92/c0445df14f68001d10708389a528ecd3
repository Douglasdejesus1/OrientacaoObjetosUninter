package uninter;

public class Euro extends Moeda{
	

	public Euro(double valorI) {
		//super(valor);
		this.valor = valorI;
	}

	@Override
	public void info() {
		System.out.printf("Euro= £$ %.2f ]",valor);
		
	}

	@Override
	public double converter() {
		return this.valor*6;
		
	}
	@Override
	public String toString() {
		return "Euro [valor= £$ " + valor + "]";
	}
	

}
