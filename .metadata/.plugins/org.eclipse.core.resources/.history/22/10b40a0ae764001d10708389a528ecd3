package empresa;

public class Principal {

	public static void main(String[] args) {
		
		Conta cDouglas = new Conta(12, "Douglas", 200, 250);
		Conta cRodrigo = new Conta(36, "Rodrigo", 0, 1000);
		cDouglas.info();
		cRodrigo.info();
		
		cDouglas.tranferir(100, cDouglas, cRodrigo);
		
		cDouglas.info();
		cRodrigo.info();
		
	
	}

}
