package empresa;

public class Principal {

	public static void main(String[] args) {
		
		Conta cDouglas = new Conta(12, "Douglas", 200, 250);
		cDouglas.info();
		
		cDouglas.sacar(300);
		cDouglas.info();

		cDouglas.sacar(100);
		cDouglas.info();
		cDouglas.depositar(300);
		cDouglas.info();
	}

}
