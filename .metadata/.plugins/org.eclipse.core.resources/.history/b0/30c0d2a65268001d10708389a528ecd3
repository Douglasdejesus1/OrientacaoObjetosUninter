package uninter;

public class Real extends Moeda{
	

	public Real(double valor) {
		this.valor=valor;
	}

	public double converter() {
		return this.valor;
	}

	@Override
	public void info() {
		System.out.printf("R$ %.2f \n",valor);
		
	}

	
	
	@Override
	public String toString() {
		return "Real [valor= R$ " + valor + "]";
	}
	
	

}
